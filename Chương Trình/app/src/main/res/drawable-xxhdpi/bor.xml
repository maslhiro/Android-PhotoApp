<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:width="300dp"
        android:height="450dp">

    <shape

        android:shape="rectangle">
        <stroke android:width="1dp" android:color="@color/darkBlue" />


    </shape>
    </item>

    <item
        android:height="300dp"
        android:width="300dp"
        >
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/darkBlue"
                />
        </shape>
    </item>
    <item
        android:height="600dp"
        android:width="300dp"
       >
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/darkBlue"
                />
        </shape>
    </item>
    <item
        android:height="900dp"
        android:width="300dp"
        >
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="@color/darkBlue"
                />
        </shape>
    </item>

    <item
        android:height="150dp"
        android:width="450dp">
        <rotate android:fromDegrees="90"
            android:pivotY="260"
            android:pivotX="260">
        <shape android:shape="line">

            <stroke
                android:width="1dp"
                android:color="@color/darkBlue"
                />

        </shape>
        </rotate>
  </item>
    <item
        android:height="150dp"
        android:width="450dp">
        <rotate android:fromDegrees="90"
            android:pivotY="410"
            android:pivotX="410">
            <shape android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="@color/darkBlue"
                    />

            </shape>
        </rotate>
    </item>

</layer-list>